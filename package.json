{
  "name": "curriculum-vitae-v2",
  "version": "1.0.0",
  "description": "Curriculum vitae of David de Lusenet",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && gatsby build",
    "develop": "gatsby develop --open",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "check:js": "eslint \"./src/**\"",
    "check:types": "tsc",
    "check:all": "npm run check:js && npm run check:types",
    "update:style": "prettier --write ."
  },
  "author": "David de Lusenet",
  "license": "MIT",
  "dependencies": {
    "@fontsource/open-sans": "^4.5.14",
    "@fontsource/source-serif-pro": "^4.5.9",
    "babel-plugin-styled-components": "^2.0.7",
    "gatsby": "^4.20.0",
    "gatsby-plugin-layout": "^3.20.0",
    "gatsby-plugin-manifest": "^4.20.0",
    "gatsby-plugin-react-helmet": "^5.20.0",
    "gatsby-plugin-react-svg": "^3.1.0",
    "gatsby-plugin-styled-components": "^5.20.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.6",
    "styled-media-query": "^2.1.2",
    "styled-reset-advanced": "^1.0.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
