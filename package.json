    "gatsby-plugin-react-helmet": "^6.14.0",
    "gatsby-plugin-react-svg": "^3.3.0",
    "gatsby-plugin-styled-components": "^6.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
